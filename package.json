{
  "name": "server",
  "version": "0.0.1",
  "description": "API Server with node express babel and eslint",
  "main": "./build/server.js",
  "scripts": {
    "devstart": "cross-env NODE_ENV=development  ./node_modules/.bin/babel-watch server/server.js",
    "start": "cross-env NODE_ENV=development node server/build/server.js",
    "build": "./node_modules/.bin/babel -d build",
    "dist": "./node_modules/.bin/babel -d dist --presets=babel-preset-minify",
    "test": "./node_modules/.bin/mocha  server/test/**/**spec.js --exit --timeout5000 --compilers js:babel-core/register --reporter spec",
    "lint": "./node_modules/.bin/eslint",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "nyc npm test&& nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiernomoudou/More-Recipes.git"
  },
  "keywords": [
    "nodejs",
    "es6",
    "es2015",
    "babel",
    "mocha",
    "lint"
  ],
  "author": "Thierno Souleymane Diallo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiernomoudou/More-Recipes.git/issues"
  },
  "homepage": "https://github.com/thiernomoudou/More-Recipes.git#readme",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-watch": "^2.0.7",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.1",
    "ejs": "^2.5.7",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "node-uuid": "^1.4.8",
    "nyc": "^11.3.0",
    "pg": "^7.4.0",
    "pg-hstore": "^2.3.2",
    "power-assert": "^1.4.4",
    "redis": "^2.8.0",
    "redis-commands": "^1.3.1",
    "sequelize": "^4.23.2",
    "swagger-tools": "^0.10.3",
    "then-redis": "^2.0.1"
  },
  "devDependencies": {},
  "engines": {
    "node": "9.2.0"
  }
}
