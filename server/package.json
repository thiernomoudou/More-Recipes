{
  "name": "server",
  "version": "0.0.1",
  "description": "API Server with node express babel and eslint",
  "main": "./build/server.js",
  "scripts": {
    "start": "babel-watch src/server.js",
    "build": "./node_modules/.bin/babel src -d build",
    "dist": "./node_modules/.bin/babel src -d dist --presets=babel-preset-minify",
    "test": "./node_modules/.bin/mocha --compilers js:babel-core/register --reporter spec",
    "lint": "./node_modules/.bin/eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thiernomoudou/More-Recipes.git"
  },
  "keywords": [
    "nodejs",
    "es6",
    "es2015",
    "babel",
    "mocha",
    "lint"
  ],
  "author": "Thierno Souleymane Diallo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thiernomoudou/More-Recipes.git/issues"
  },
  "homepage": "https://github.com/thiernomoudou/More-Recipes.git#readme",
  "dependencies": {
    "body-parser": "^1.18.2",
    "express": "^4.16.2",
    "node-uuid": "^1.4.8"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-minify": "^0.2.0",
    "babel-watch": "^2.0.7",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "mocha": "^3.4.2",
    "power-assert": "^1.4.4"
  }
}
